<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SweepSelect">
        <attr name="itemString" format="reference" />
        <attr name="selectedColor" format="reference" />
        <attr name="normalColor" format="reference" />
        <attr name="selectedSize" format="dimension" />
        <attr name="normalSize" format="dimension" />
        <attr name="singleLine" format="boolean" />
        <attr name="numberEachLine" format="integer" />
        <attr name="spaceEachLine" format="dimension" />
        <attr name="emptyPrefix" format="integer" />
        <attr name="chooseMode" format="enum">
            <enum name="multySweep" value="0" />
            <enum name="singleSweep" value="1" />
            <enum name="singleTap" value="2" />
        </attr>
    </declare-styleable>

</resources>